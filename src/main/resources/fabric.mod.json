{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}+${minecraft_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}\n\n"
  ],
  "contributors": [
    "Hexasan\n\n"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/${modrinth_slug}",
    "sources": "https://github.com/${github_repo}"
  },
  "license": "${license}",
  "icon": "${mod_id}.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "sylenthuntress.unbreakable.util.Unbreakable"
    ],
    "client": [
      "sylenthuntress.unbreakable.client.UnbreakableClient"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=21",
    "fabric-api": "${fabric_version}",
    "owo-lib": "${owo_version}"
  },
  "recommends": {
    "modmenu": ">=${modmenu_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "unbreakable.discord": "${discord_invite}"
      },
      "update_checker": true
    }
  }
}